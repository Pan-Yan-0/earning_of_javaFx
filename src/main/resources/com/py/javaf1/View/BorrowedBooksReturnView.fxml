<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.py.javaf1.Controller.BorrowedBooksReturnController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="borrowedBooksTable" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="bookIDColumn" text="图书编号" />
                <TableColumn fx:id="titleColumn" text="图书名称" />
                <TableColumn fx:id="authorColumn" text="作者" />
                <TableColumn fx:id="borrowDateColumn" text="借阅时间" />
                <TableColumn fx:id="returnButtonColumn" text="操作" />
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets top="10" right="10" bottom="10" left="10" />
    </padding>
</VBox>
