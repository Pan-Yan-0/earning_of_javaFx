<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.py.javaf1.Controller.BookRankController">
    <TableView fx:id="rankingTable" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="bookIDColumn" text="书籍Id" prefWidth="100.0"/>
            <TableColumn fx:id="titleColumn" text="书籍名称" prefWidth="200.0"/>
            <TableColumn fx:id="authorColumn" text="作者" prefWidth="150.0"/>
            <TableColumn fx:id="borrowCountColumn" text="借阅次数" prefWidth="100.0"/>
        </columns>
    </TableView>
</AnchorPane>
