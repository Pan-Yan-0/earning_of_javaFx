<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.py.javaf1.Controller.HistoryController">
    <children>
        <TableView fx:id="historyTable" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="readerIDColumn" text="读者编号" />
                <TableColumn fx:id="bookIDColumn" text="图书编号" />
                <TableColumn fx:id="borrowDateColumn" text="借阅时间" />
                <TableColumn fx:id="returnDateColumn" text="归还时间" />
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets top="10" right="10" bottom="10" left="10" />
    </padding>
</VBox>
